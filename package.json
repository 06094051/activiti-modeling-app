    "@angular/cli": "^6.1.5",
    "@angular/compiler-cli": "^6.1.4",
    "@angular/language-service": "^6.1.4",
    "@ngrx/schematics": "^6.1.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.113",
    "ajv-cli": "^3.0.0",
    "alfresco-js-api-node": "2.7.0-beta1",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "4.1.0",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "dateformat": "^3.0.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jasmine-core": "~3.2.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine2-protractor-utils": "^1.3.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-preset-angular": "^5.2.3",
    "ng-packagr": "^4.1.0",
    "rimraf": "^2.6.2",
    "rxjs-tslint-rules": "^4.4.4",
    "ts-node": "~4.1.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "2.9.2",
    "webpack-cli": "^3.1.0"
  }
}
