{
  "name": "activiti-modeling-application",
  "description": "Activiti Modeling Application",
  "version": "0.2.0",
  "author": "Alfresco Software, Ltd.",
  "scripts": {
    "ng": "ng",
    "precommit": "lint-staged",
    "01": "echo --------------------------------------- Community servers ------------------------------------------",
    "prestart": "npm run validate-config && npm run compile-less && npm run package:sdk",
    "start": "npm run server-versions && rimraf dist && ng serve --host 0.0.0.0 dist --open",
    "prestart:dev": "npm run validate-config && npm run compile-less ",
    "start:dev": " npm run server-versions && rimraf dist && npm run clean-lib-angular && concurrently --success first \"ng serve --host 0.0.0.0 --disable-host-check dev --open\" \"npm run style:dev --watch\" \"npm run copy:dev\" ",
    "prestart:dist": "npm run prestart",
    "start:dist": "npm run server-versions && rimraf dist && node --max_old_space_size=30000 node_modules/.bin/ng serve --prod --host 0.0.0.0 --disable-host-check dist",
    "02": "echo --------------------------------------- Community builders -----------------------------------------",
    "prebuild": "npm run compile-less && npm run package:sdk",
    "build": "npm run validate-config && npm run server-versions && rimraf dist && ng build dist",
    "prebuild:dev": "npm run compile-less",
    "build:dev": "npm run validate-config && npm run lint && npm run style:dev && npm run server-versions && rimraf dist && node --max_old_space_size=30000 node_modules/.bin/ng build dev",
    "prebuild:dist": "npm run prebuild",
    "build:dist": "npm run validate-config && npm run server-versions && rimraf dist && node --max_old_space_size=30000 node_modules/.bin/ng build --prod dist",
    "03": "echo -------------------------------------------- AMA SDK -----------------------------------------------",
    "build:sdk": "ng build --prod ama-sdk",
    "pack:sdk": "cd dist-sdk/ama-sdk && npm pack",
    "package:sdk": "npm run build:sdk && npm run pack:sdk",
    "node-modules-sdk:clean": "rimraf node_modules/ama-sdk",
    "node-modules-sdk:copy": "mkdir -p ./node_modules/ama-sdk && cp -R ./dist-sdk/ama-sdk/* ./node_modules/ama-sdk",
    "node-modules-sdk": "npm run node-modules-sdk:clean && npm run node-modules-sdk:copy",
    "04": "echo ------------------------------------------- Unit tests ---------------------------------------------",
    "pretest": "rimraf dist-sdk",
    "test": "jest --notify --config jest.config.json",
    "pretest:watch": "npm run pretest",
    "test:watch": "npm test -- --watch",
    "pretest:ci": "npm run pretest",
    "test:ci": "node --max_old_space_size=30000 ./node_modules/jest/bin/jest.js --config jest.config.json --collectCoverage",
    "05": "echo -------------------------------------- All the other stuff -----------------------------------------",
    "style:dev": "npm run webpack -- --config  config/webpack.style.js --progress --profile --bail",
    "lint": "ng lint",
    "githooks": "cp ./githooks/* .git/hooks/; chmod 0755 .git/hooks/*",
    "copy:dev": "node ./config/dev-copy-watch.js",
    "validate-config": "ajv validate -s ./node_modules/@alfresco/adf-core/app.config.schema.json -d ./src/app.config.json --errors=text --verbose",
    "server-versions": "rimraf ./src/versions.json && npm list --depth=0 --json=true --prod=true > ./src/versions.json  || exit 0",
    "clean": "rimraf dist dist-sdk dist-dev-temp lib node_modules typings",
    "clean-lib-angular": "rimraf ../alfresco-ng2-components/lib/node_modules/@angular",
    "clean-lock": "rimraf package-lock.json",
    "webpack": "node node_modules/webpack/bin/webpack.js",
    "compile-less": "lessc ./node_modules/bpmn-js-properties-panel/styles/properties.less .tmp/bpmn-properties.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Activiti/activiti-modeling-app.git"
  },
  "lint-staged": {
    "*.ts": [
      "tslint",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/Activiti/activiti-modeling-app/issues"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Eugenio Romano",
      "email": "eugenio.romano@alfresco.com"
    },
    {
      "name": "Mario Romano",
      "email": "mario.romano83@gmail.com"
    },
    {
      "name": "Andras Popovics",
      "email": "popovics@ndras.hu"
    }
  ],
  "keywords": [
    "ng2",
    "angular",
    "angular2",
    "alfresco"
  ],
  "private": true,
  "dependencies": {
    "@alfresco/adf-core": "2.7.0-beta2",
    "alfresco-js-api": "2.7.0-beta2",
    "@angular/animations": "6.1.4",
    "@angular/cdk": "^6.4.6",
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "6.1.4",
    "@angular/http": "6.1.4",
    "@angular/material": "^6.4.6",
    "@angular/material-moment-adapter": "^6.4.6",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@angular/router": "6.1.4",
    "@mat-datetimepicker/core": "^2.0.1",
    "@mat-datetimepicker/moment": "^2.0.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@ngx-translate/core": "10.0.2",
    "@types/lodash": "^4.14.112",
    "ajv": "^6.5.2",
    "bpmn-js": "^1.3.0",
    "bpmn-js-properties-panel": "^0.25.1",
    "camunda-bpmn-moddle": "^3.0.0",
    "classlist.js": "1.1.20150312",
    "codecov": "^3.0.4",
    "core-js": "2.4.1",
    "custom-event-polyfill": "0.3.0",
    "hammerjs": "2.0.8",
    "husky": "^0.14.3",
    "intl": "1.2.5",
    "jasmine-marbles": "^0.3.1",
    "lint-staged": "^7.2.2",
    "lodash": "^4.17.10",
    "minimatch": "3.0.4",
    "minimatch-browser": "^1.0.0",
    "moment": "2.22.2",
    "moment-es6": "^1.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-moment": "^2.0.0",
    "ngx-monaco-editor": "6.0.0",
    "pdfjs-dist": "2.0.303",
    "reflect-metadata": "0.1.10",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
